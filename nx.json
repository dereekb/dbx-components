{
  "affected": {
    "defaultBase": "develop"
  },
  "cli": {
    "packageManager": "npm"
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["production", "^production"],
      "cache": true
    },
    "run-all-tests": {
      "dependsOn": ["build"],
      "inputs": ["default", "^default", "{workspaceRoot}/jest.preset.ts"]
    },
    "test": {
      "dependsOn": ["build"],
      "inputs": ["default", "^default", "{workspaceRoot}/jest.preset.ts"],
      "cache": true
    },
    "publish-npmjs": {
      "dependsOn": ["build"]
    },
    "deploy": {
      "dependsOn": ["build"]
    },
    "ci-deploy": {
      "dependsOn": ["build"]
    },
    "e2e": {
      "inputs": ["default", "^production"],
      "cache": true
    },
    "build-base": {
      "cache": true
    },
    "@nx/jest:jest": {
      "cache": true,
      "inputs": ["default", "^production"],
      "options": {
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "@nx/eslint:lint": {
      "inputs": ["default", "{workspaceRoot}/.eslintrc.json"],
      "cache": true
    }
  },
  "generators": {
    "@nx/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nx/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nx/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "demo",
  "workspaceLayout": {
    "appsDir": "apps",
    "libsDir": "packages"
  },
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "pluginsConfig": {
    "@nx/js": {
      "analyzeSourceFiles": true
    }
  },
  "namedInputs": {
    "default": ["{projectRoot}/**/*", "sharedGlobals"],
    "sharedGlobals": [],
    "production": ["default", "!{projectRoot}/.eslintrc.json", "!{projectRoot}/**/?(*.)+(spec|test).[jt]s?(x)?(.snap)", "!{projectRoot}/tsconfig.spec.json", "!{projectRoot}/jest.config.[jt]s", "!{projectRoot}/src/test-setup.[jt]s"]
  },
  "nxCloudAccessToken": "OWU3ZTBhOWYtYTI1MS00NTgyLTk5MWItOTNlYjQ4YTFiOWZkfHJlYWQtd3JpdGU="
}
