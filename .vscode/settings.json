{
  "typescript.preferences.quoteStyle": "single",
  "typescript.tsdk": "node_modules/typescript/lib",
  "search.followSymlinks": false,
  "files.watcherExclude": {
    "**/.angular/**": true,
    "**/.reports/**": true,
    "**/.nx/cache/*/**": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/dist/**": true,
    "**/tmp/**": true,
    "**/.hg/store/**": true
  },
  "eslint.validate": ["json"]
}
